@model Sistema_de_Informes_de_Analisis_Financieros.Models.Ratiobasesector

@{
    ViewData["Title"] = "Delete";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Eliminar</h1>

<h3>¿Está seguro de eliminar este registro</h3>
<div>   
    <dl class="row">
        <dt class = "col-sm-2">
            Valor:
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Valorratiob)
        </dd>
        <dt class = "col-sm-2">
            Razón:
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.IdratioNavigation.Nombreratiob)
        </dd class>
        <dt class = "col-sm-2">
            Sector:
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.IdsectorNavigation.Nomsector)
        </dd class>
    </dl>
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="Idratio" />
        <input type="hidden" asp-for="Idsector" />
        <input type="submit" value="Eliminar" class="btn btn-danger" /> |
        <a asp-action="Index" class="btn btn-success">Volver a la lista</a>
    </form>
</div>
