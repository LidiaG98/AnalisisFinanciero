@model IEnumerable<Sistema_de_Informes_de_Analisis_Financieros.Models.Valoresestado>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Nombrevalore)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Valorestado)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Anio)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Id)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Nombrevalore)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Valorestado)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Anio)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Id.Codcuentacatalogo)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Idvalore">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Idvalore">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Idvalore">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
